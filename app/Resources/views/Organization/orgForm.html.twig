<p><strong>Organization</strong></p>
<div id="orgNotice"></div>
{% if app.user is null %}
    {% set form = form.organization %}
{% endif %}
<div id="orgForm">
    <div class="col-md-12">
        {{ form_row(form.orgName) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.address) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.city) }}
        {{ form_row(form.state) }}
        {{ form_row(form.zip) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.areacode) }}
        {{ form_row(form.phone) }}
    </div>
    <div class="col-md-12">
        {{ form_row(form.email) }}
    </div>
    <div class="col-md-12">
        <div class="input-group">
            <div class="input-group-addon">http://</div>
            {{ form_row(form.website) }}
        </div>
    </div>
    <div class="col-md-12">
        Background check req'd {{ form_widget(form.background) }}
    </div>
</div>